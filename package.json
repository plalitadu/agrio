{
  "name": "agrio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-list": "emulator -list-avds",
    "android-clean": "cd android && ./gradlew clean",
    "android-dev": "react-native run-android --variant=devDebug",
    "android-stg": "react-native run-android --variant=stgDebug",
    "android-qa": "react-native run-android --variant=qaDebug",
    "android-prod": "react-native run-android --variant=prodDebug",
    "ios": "react-native run-ios --simulator=\"iPhone 14\"",
    "ios-dev": "react-native run-ios --simulator=\"iPhone 14\" --scheme \"agrioEnvDev\"",
    "ios-stg": "react-native run-ios --simulator=\"iPhone 14\" --scheme \"agrioEnvStg\" ",
    "ios-prod": "react-native run-ios --simulator=\"iPhone 14\" --scheme \"agrioEnvProd\" ",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@fontsource/ibm-plex-sans": "^4.5.13",
    "@react-navigation/bottom-tabs": "^6.5.5",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "@types/react-native": "^0.71.0",
    "babel-plugin-root-import": "^6.6.0",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-config": "^1.4.12",
    "react-native-paper": "^5.1.4",
    "react-native-password-eye": "^1.0.5",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.8.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-vector-icons": "^9.2.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/runtime": "^7.20.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.32.0",
    "jest": "^29.3.1",
    "metro-react-native-babel-preset": "^0.74.0",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
